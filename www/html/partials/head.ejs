<head>
  <title>Modder of the Year</title>

  <meta property="og:type" content="website">
  <meta property="og:title" content="MOTY">
  <meta property="og:description" content="<%= metaGameName %> - Modder of the Year">
  <meta property="og:image" content="<%= metaImage %>">

  <meta name="viewport" content="width=device-width, initial-scale=1">

  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
  <script src="https://code.jquery.com/color/jquery.color.plus-names-2.1.2.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/FitText.js/1.2.0/jquery.fittext.min.js"></script>
  <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>

  <link rel="stylesheet" href="https://code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">

  <link rel="shortcut icon" href="<%= metaImage %>">
  <link rel="icon" href="<%= metaImage %>">

  <style>
    html, body {
      background-color: #181a1b;
      color: white;
      font-family: "Helvetica Neue", sans-serif;
      margin: 0;
    }

    a:not(.notalink) {
      color: #3391ff;
    }

    div[role="tooltip"] {
      color: white;
      background-color: #333;
      text-align: justify;
    }

    ::-webkit-scrollbar {
      background-color: #1c1e1f;
    }

    ::-webkit-scrollbar-corner {
      background-color: #181a1b;
    }

    ::-webkit-scrollbar-thumb {
      background-color: #2a2c2e;
    }

    * {
      scrollbar-color: #2a2c2e #1c1e1f;
    }
  </style>

  <script>
    $(function(){
      $(document).tooltip();
    });

    function parseQueryString() {
      var objURL = {};
      window.location.search.replace(
        new RegExp("([^?=&]+)(=([^&]*))?", "g"),
        function($0, $1, $2, $3){
          objURL[$1] = $3;
        },
      );
      return objURL;
    };

    function parseSearchString() {
      var objURL = [];
      window.location.hash.replace(
        new RegExp("([^#]+)", "g"),
        function($0, $1){
          objURL.push($1);
        },
      );
      return objURL;
    };

    if (parseQueryString().alert) {
      alert(unescape(parseQueryString().alert));
    }

    if (parseQueryString().server) {
      alert("You need to be in our Discord server in order to vote.");
      window.location.href = "https://discord.gg/UpWuWwq";  
    }

    $.when($.ready).then(() => {
      setInterval(() => {
        if ($(window).width() < 1230) $("body").addClass("mobile");
        else $("body").removeClass("mobile");
      })
    });

    $.fn.exists = function () {
      return this.length !== 0;
    }
  </script>
</head>